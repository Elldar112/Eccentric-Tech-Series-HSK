  <!-- M61 PGM (Antigrain) -->
  <ThingDef ParentName="EccentricDefenseGrid_BaseArtilleryProjectile">
    <defName>EccentricDefenseGrid_Projectile_M61PGM</defName>
    <label>M61 precision guided missile</label>
    <graphicData>
      <texPath>Eccentric/Things/Projectiles/Projectile_M61_Green</texPath>
    </graphicData>
    <projectile>
      <damageDef>BombSuper</damageDef>
      <speed>40</speed>
      <explosionRadius>14.9</explosionRadius>
      <explosionChanceToStartFire>0.22</explosionChanceToStartFire>
      <explosionDamageFalloff>true</explosionDamageFalloff>
      <explosionEffect>GiantExplosion</explosionEffect>
      <soundHitThickRoof>Artillery_HitThickRoof</soundHitThickRoof>
      <soundExplode>Explosion_GiantBomb</soundExplode>
      <soundImpactAnticipate>MortarRound_PreImpact</soundImpactAnticipate>
      <soundAmbient>MortarRound_Ambient</soundAmbient>
      <shadowSize>0.6</shadowSize>
      <arcHeightFactor>12</arcHeightFactor>
      <flyOverhead>true</flyOverhead>
    </projectile>
    <modExtensions>
      <li Class="EccentricProjectiles.ModExtension_ProjectileEffects">
        <effects>
          <li>Eccentric_ArtilleryMissileTrail</li>
          <li>Eccentric_ArtilleryMissileSmoke</li>
          <li>Eccentric_ArtilleryMissileExhaust</li>
        </effects>
        <impactEffects>
          <li>Eccentric_ArtilleryImpactImplosion</li>
        </impactEffects>
        <stages>
