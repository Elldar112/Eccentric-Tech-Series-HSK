<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="EccentricDefenseGrid_BaseArtilleryProjectile" ParentName="BaseBullet" Abstract="True">
    <thingClass>EccentricProjectiles.Projectile_ExplosiveStagedWithEffects</thingClass>
    <graphicData>
      <texPath>Eccentric/Things/Projectiles/Projectile_M36</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>TransparentPostLight</shaderType>
      <drawSize>1</drawSize>
    </graphicData>
    <drawOffscreen>true</drawOffscreen>
  </ThingDef>

  <!-- M36 PGM (HE) -->
  <ThingDef ParentName="EccentricDefenseGrid_BaseArtilleryProjectile">
    <defName>EccentricDefenseGrid_Projectile_M36PGM</defName>
    <label>M36 precision guided missile</label>
    <graphicData>
      <texPath>Eccentric/Things/Projectiles/Projectile_M36_Red</texPath>
    </graphicData>
    <projectile>
      <damageDef>Bomb</damageDef>
      <damageAmountBase>60</damageAmountBase>
      <explosionRadius>4.9</explosionRadius>
      <speed>40</speed>
      <shadowSize>0.6</shadowSize>
      <arcHeightFactor>12</arcHeightFactor>
      <flyOverhead>true</flyOverhead>
    </projectile>
    <modExtensions>
      <li Class="EccentricProjectiles.ModExtension_ProjectileEffects">
        <effects>
          <li>Eccentric_ArtilleryMissileTrail</li>
          <li>Eccentric_ArtilleryMissileSmoke</li>
          <li>Eccentric_ArtilleryMissileExhaust</li>
        </effects>
        <impactEffects>
          <li>Eccentric_ArtilleryImpactDistortion</li>
        </impactEffects>
        <stages>
          <li>
            <label>ejected</label>
            <type>Launch</type>
            <duration>24</duration>
            <overrideInitialAngle>true</overrideInitialAngle>
            <angle>-90</angle>
            <initialHeight>0.1</initialHeight>
            <heightOffset>2</heightOffset>
            <heightFunction>EaseOutQuad</heightFunction>
            <startSound>Eccentric_ArtilleryMissileEject</startSound>
          </li>
          <li>
            <label>tracking</label>
            <type>Terminal</type>
            <speed>40</speed>
            <positionFunction>EaseInCubic</positionFunction>
            <arcFactor>12</arcFactor>
            <startSound>Eccentric_ArtilleryMissileIgnite</startSound>
          </li>
        </stages>
      </li>
    </modExtensions>
  </ThingDef>

