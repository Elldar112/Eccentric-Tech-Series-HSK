          </li>
          <li>
            <label>tracking</label>
            <type>Terminal</type>
            <speed>40</speed>
            <positionFunction>EaseInCubic</positionFunction>
            <arcFactor>12</arcFactor>
            <startSound>Eccentric_ArtilleryMissileIgnite</startSound>
          </li>
        </stages>
      </li>
    </modExtensions>
  </ThingDef>

